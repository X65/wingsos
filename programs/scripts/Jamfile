TOP ?= ../.. ;
SubDir TOP programs scripts ;
LOCATE_TARGET = $(SUBDIR)/obj ;

SetBinDir $(BINS)scripts ;

CopyBin passwd uptime dodisk remount dial ;

SetBinDir $(BINS)programs/net ;

CopyBin guestbook.an ;

SetBinDir $(BINS)gui ;

if $(DBGWGS)
{
    CopyBin debug/gui ;
}
else
{
    CopyBin gui ;
}

SetBinDir $(BINS)system ;

if $(DBGWGS)
{
   CopyBin debug/init ;
}
else
{
   CopyBin init ;
}
